import matplotlib.pyplot as plt
x1 = [10, 20, 30]
y1 = [20, 40, 10]
x2 = [10, 20, 30]
y2 = [40, 10, 30]
plt.figure(figsize=(10, 4))
plt.subplot(1, 2, 1)
plt.plot(x1, y1, 'b-')
plt.title('Plot 1')
plt.xlabel('x - axis')
plt.ylabel('y - axis')
plt.subplot(1, 2, 2)
plt.plot(x2, y2, 'r-')
plt.title('Plot 2')
plt.xlabel('x - axis')
plt.ylabel('y - axis')
plt.tight_layout()
plt.show()
