import pandas as pd
import matplotlib.pyplot as plt

# Create historical stock price data for Alphabet Inc. (GOOGL)
data = {
    "Date": [
        "2023-01-02", "2023-01-03", "2023-01-04",
        "2023-01-05", "2023-01-06", "2023-01-09",
        "2023-01-10"
    ],
    "Close": [
        88.73, 89.12, 87.95,
        89.58, 90.73, 91.89,
        92.32
    ]
}

# Create DataFrame
df = pd.DataFrame(data)

# Convert Date column to datetime
df["Date"] = pd.to_datetime(df["Date"])

# Set Date as index
df.set_index("Date", inplace=True)

# Specify date range
start_date = "2023-01-03"
end_date = "2023-01-10"

# Filter data between the specified dates
filtered_df = df.loc[start_date:end_date]

# Plot the closing stock prices
plt.figure(figsize=(8, 4))
plt.plot(filtered_df.index, filtered_df["Close"])
plt.title("Alphabet Inc. (GOOGL) Stock Prices")
plt.xlabel("Date")
plt.ylabel("Closing Price (USD)")
plt.grid(True)

# Display the plot
plt.show()


