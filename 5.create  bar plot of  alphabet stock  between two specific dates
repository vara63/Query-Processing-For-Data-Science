import pandas as pd
import matplotlib.pyplot as plt
data = {
    'Date': pd.date_range(start='2024-01-01', end='2024-01-07'),
    'Volume': [1800000, 2200000, 2000000, 2500000, 2300000, 2100000, 2600000]
}
df = pd.DataFrame(data)
start_date = '2024-01-02'
end_date = '2024-01-06'
filtered_df = df[(df['Date'] >= start_date) & (df['Date'] <= end_date)]
plt.figure()
plt.bar(filtered_df['Date'].dt.strftime('%Y-%m-%d'), filtered_df['Volume'])
plt.xlabel('Date')
plt.ylabel('Trading Volume')
plt.title('Alphabet Inc. Trading Volume')
plt.show()
